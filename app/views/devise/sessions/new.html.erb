<div class="main">
  <div class="user-main">
    <div class="user-title">
      <h2>ログイン</h2>
    </div>
      <%= form_with model: resource, as: resource_name, url: session_path(resource_name), local: true do |f|%>
        <div class='field'>
            <%= f.label :メールアドレス%>
            <%= f.email_field :email, autofocus: true,class: "input"%>
        </div>
        <div class='field'>
          <%= f.label :パスワード%>
          <%= f.password_field :password, autocomplete: "off",class: "input"%>
        </div>
          <div class='actions'>
            <%= f.submit "ログイン", class: 'btn'%>
          </div>
      <% end %>
      <%= render "devise/shared/links"%>
  </div>
</div>
